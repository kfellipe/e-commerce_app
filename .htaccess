RewriteEngine On

RewriteCond %{REQUEST_FILENAME}$ !-f
RewriteCond %{REQUEST_FILENAME}$ !-d
RewriteRule ^([a-z,A-Z,0-9,_-])$ index.php

Options All -Indexes

RewriteRule ^(meus-anuncios)/?$ /Viewer/pages/product/products.php [NC]
RewriteRule ^(atualizar)/([a-z,A-Z,0-9]+)/?$ /Viewer/pages/update.php?nome=$2 [NC]
RewriteRule ^(perfil)/([a-z,A-Z,0-9]+)/?$ /Viewer/pages/perfil.php?nome=$2 [NC]
RewriteRule ^(deletar)/([a-z,A-Z,0-9]+)/?$ /Viewer/pages/delete.php?nome=$2 [NC]
RewriteRule ^/?(teste)/?$ /Viewer/pages/teste.php [NC]
RewriteRule ^/?(registrar)/?$ /Viewer/pages/registrar.php [NC]
RewriteRule ^/?(home)/?$ /Viewer/pages/home.php [NC]
RewriteRule ^/?(login)/?$ /Viewer/pages/login.php [NC]
ErrorDocument 403 /Viewer/pages/errors/403.php
ErrorDocument 404 /Viewer/pages/errors/404.php